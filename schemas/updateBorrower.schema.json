{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "$id": "http://example.com/example.json",
  "type": "object",
  "default": {},
  "title": "Root Schema",
  "required": ["firstName", "lastName", "phone"],
  "properties": {
    "firstName": {
      "type": "string",
      "default": "",
      "title": "The firstName Schema",
      "examples": ["bobbbbbbbb"]
    },
    "lastName": {
      "type": "string",
      "default": "",
      "title": "The lastName Schema",
      "examples": ["Smith"]
    },
    "phone": {
      "type": "integer",
      "default": 0,
      "title": "The phone Schema",
      "examples": [1234567890]
    }
  },
  "examples": [
    {
      "firstName": "bobbbbbbbb",
      "lastName": "Smith",
      "phone": 1234567890
    }
  ]
}
